struct FieldTypeObjectReference {
  object:TypespaceObject;
}

struct FieldTypePrimitive {
  provider:string;
}

oneof FieldType {
  FieldTypeObjectReference,
  FieldTypePrimitive
}

object TypespaceObject {
  title:string;
  position:[int32, int32];
}

object TypespaceObjectField {
  title:string;
  object:TypespaceObject;
  type:FieldType;
  optional:bool;
}
