struct FieldTypeObjectReference {
  obj:TypespaceObject;
}

struct FieldTypePrimitive {
  provider:string;
}

oneof FieldType {
  FieldTypeObjectReference,
  FieldTypePrimitive
}

object TypespaceObject {
  title:string;
  position:[i32, i32];
  
  @View(TypespaceObjectField, obj) fields;
}

object TypespaceObjectField {
  title:string;
  obj:TypespaceObject;
  type:FieldType;
  optional:bool;
}
